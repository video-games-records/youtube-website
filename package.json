{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "build-staging": "vite build --mode=staging",
    "build-production": "vite build --mode=production",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write src/",
    "pwa:build": "vite build --mode=production",
    "pwa:preview": "vite preview --host",
    "pwa:analyze": "vite-bundle-analyzer dist",
    "pwa:test": "start-server-and-test 'npm run preview' http://localhost:4173 'echo PWA test server running'",
    "icons:generate": "pwa-asset-generator public/logo.svg public --background '#ffffff' --theme-color '#ffffff'",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html --chrome-flags='--headless'"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "@unhead/vue": "^2.0.17",
    "@vee-validate/zod": "^4.15.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.503.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "quill": "^2.0.3",
    "reka-ui": "^2.3.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.8",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-gtag": "^3.5.0",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1",
    "vue-sonner": "^1.3.2",
    "workbox-window": "^7.3.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.15.2",
    "@types/quill": "^2.0.14",
    "@types/vue-i18n": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^10.1.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vite-plugin-pwa": "^1.0.0",
    "vue-tsc": "^2.2.8"
  }
}
